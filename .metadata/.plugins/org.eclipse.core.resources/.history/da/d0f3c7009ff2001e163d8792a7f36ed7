package com.example.demo.Controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MainController {
	@GetMapping("/")
	public String hello() {
		return "home";
	}
	@GetMapping("/login")
	public String gotoLoginPage() {
		return "login";
	}
	@GetMapping("/About")
	public String about() {
		return "About";
	}
}